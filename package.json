{
  "name": "bundlelook",
  "version": "2.0.2",
  "description": "Here is the lookup for bundle content",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "prepare": "tsup src/index.ts --format esm,cjs",
    "build": "tsup src/index.ts --format esm,cjs --dts-resolve",
    "test": "uvu -r tsm tests test.ts",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:brandonxiang/bundlelook.git"
  },
  "keywords": [
    "bundle information"
  ],
  "author": "brandonxiang",
  "license": "MIT",
  "dependencies": {
    "fast-glob": "^3.2.7",
    "sade": "^1.7.4"
  },
  "devDependencies": {
    "tsup": "^5.9.1",
    "typescript": "^4.5.3",
    "@types/node": "^16.11.10",
    "tsm": "^2.2.1",
    "uvu": "^0.5.2",
    "release-it": "^17.10.0"
  },
  "bin": {
    "bundlelook": "bin.js"
  }
}
